{
  "name": "dwnler",
  "version": "1.0.0",
  "description": "Electron application to convert and download music as MP3.",
  "main": "src/index.js",
  "scripts": {
    "dev": "NODE_ENV=dev electron src/index.js",
    "lint": "standard",
    "test": "ava tests/test-*.js",
    "start": "electron-forge start",
    "package-win": "electron-forge package --platform=win32 --arch=ia32",
    "package-linux": "electron-forge package --platform=linux --arch=x64",
    "make": "electron-forge make"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/misalabs/dwnler.git"
  },
  "keywords": [
    "electron",
    "youtube",
    "spotify",
    "mp3"
  ],
  "author": "misalabs <hi@misalabs.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/misalabs/dwnler/issues"
  },
  "homepage": "https://github.com/misalabs/dwnler#readme",
  "dependencies": {
    "electron-compile": "^6.4.4",
    "electron-debug": "^2.0.0",
    "electron-settings": "^3.2.0",
    "ffbinaries": "^1.1.1",
    "fluent-ffmpeg": "^2.1.2",
    "fs.extra": "^1.3.2",
    "sanitize-filename": "^1.6.1",
    "ytdl-core": "^0.29.1"
  },
  "devDependencies": {
    "ava": "^1.2.1",
    "devtron": "^1.4.0",
    "electron-forge": "^5.2.4",
    "electron-prebuilt-compile": "4.0.0",
    "spectron": "^5.0.0",
    "standard": "^12.0.1"
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "dmg"
        ],
        "linux": [
          "deb"
        ]
      },
      "electronPackagerConfig": {
        "asar": true
      },
      "electronWinstallerConfig": {
        "name": "Dwnler"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "git+https://github.com/misalabs/dwnler.git",
        "name": "Dwnler"
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "Dwnler"
      }
    }
  }
}
